{
  "name": "url-capture",
  "publisher": "komiyamma",
  "displayName": "URL Capture",
  "description": "Select a URL in a text editor and take a web screenshot of that URL.\nエディタで選択しているテキストをURLとみなして、そのファイルと同じフォルダに画損を保存します。",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "license": "MIT",
  "homepage": "https://github.com/komiyamma/vscode_extension_url_capture",
  "repository": {
    "type": "git",
    "url": "https://github.com/komiyamma/vscode_extension_url_capture"
  },
  "icon": "resources/icons/logo.png",
  "keywords": [
    "capture",
    "url-capture",
    "capture-url",
    "screenshot",
    "screenshot-url",
    "url-screenshot"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "url-capture.url-capture",
        "title": "Capture URL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "puppeteer": "^23.4.1"
  }
}
